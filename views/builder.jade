extends layout
  
block title 
  title Stuart's sequence Builder
  
block content
  header
    h1 New Sequence
    nav 
      a Profile
  main
    nav.elements 
      - var j = 1
      select 
        while j <= 5
          option(val=j)= j++
      - var i = 1
      while i <= 5
        div.sequence(id=i++)
          p.close X
          h2
    form(action='/sequence' method='post')
      button Save Sequence
    
      
