extends layout
  
block title 
  title Stuart's sequence Builder
  
block content
  header
    h1 New Sequence
    nav 
      a(href='/users/' + username + '/') Profile
  main.builder
    nav.elements 
      div.selectbox
        select
          each category in categories
            option(value=category.id)= category.name  
    form.builder(action='/sequence' method='post')
      button(type='button') 
        a Save Sequence
block scripts
  script.
    var username = !{JSON.stringify(username)};
    var posecategories = !{JSON.stringify(posecategories)}
  script(src='/js/builder.js')
      
